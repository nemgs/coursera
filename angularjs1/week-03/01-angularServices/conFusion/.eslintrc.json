{
    "extends": "google",
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "max-len": "off",
        "spaced-comment": "off",
        "quotes": "off",
        "object-curly-spacing": "off",
        "space-before-function-paren": "off",
        "comma-dangle": "off",
        "eol-last": "off",
        "no-var": "warn",
        "brace-style": "off"
    }
}